{
  "display_information": {
    "name": "lead-agent"
  },
  "features": {
    "bot_user": {
      "display_name": "lead-agent",
      "always_online": true
    }
  },
  "oauth_config": {
    "scopes": {
      "bot": ["chat:write"]
    }
  },
  "settings": {
    "interactivity": {
      "is_enabled": true,
      "request_url": "https://your-lead-agent-app-url.com/api/slack"
    },
    "event_subscriptions": {
      "request_url": "https://your-lead-agent-app-url.com/api/slack",
      "bot_events": []
    },
    "org_deploy_enabled": true,
    "socket_mode_enabled": false,
    "token_rotation_enabled": false
  }
}
